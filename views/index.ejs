<!DOCTYPE html>
<html lang="en">
	<head>
		<!--Including Head partial-->
		<%- include('./partials/head'); %>
	</head>
	<body class="homepage-container page">
		<!--Including Navigation partial-->
		<%- include('./partials/nav') %>
		<!--Homepage Title-->
		<div class="homepage-title title">Vanilla Dex</div>
		<div class="homepage-paths">
			<div class="homepage-selection">
				<!--Homepage link that routes to /pokedex and opens the Pokedex-->
				<a href="/pokedex">Pokedex</a>
			</div>
			<div class="homepage-selection">
				<!--Homepage link that routes to /moves and opens the Moves List-->
				<a href="/moves">Moves</a>
			</div>
		</div>
		<script>
			fetch("/pokedex/api")
				.then((response) => {
					console.log(response);
					return response.json();
				})
				.then((data) => {
					consoleLogData(data);
				})
				.catch((err) => {
					console.log(err);
				});

			console.log("hello world");

			function consoleLogData(data) {
				for (var i = 0; i < data.length; i++) {
					console.log(data[i]);
				}
			}
		</script>
	</body>
</html>
