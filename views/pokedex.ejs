<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./partials/head'); %>
	</head>
	<body>
		<%- include('./partials/nav') %>
		<div><p>Hello World!</p></div>
		<div>
			<p>pokedex.ejs</p>
		</div>
		<div class="dex-pokemon-card card">
			<% for(var i=0; i<pokemon.length;i++) { %>
				<ol class="card-info">
					<li><%= i %></li>
					<li><%= pokemon[i].information.name %></li>
					<li><%= pokemon[i].information.dexNumber %></li>
					<li><%= pokemon[i].information.primaryType %></li>
					<li><%= pokemon[i].information.description %></li>
				</ol>
				<% } %>
		</div>
	</body>
</html>
